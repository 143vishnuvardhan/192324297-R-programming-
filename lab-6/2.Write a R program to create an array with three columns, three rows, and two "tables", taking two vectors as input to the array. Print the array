vector1 <- c(1, 2, 3, 4, 5, 6)
vector2 <- c(7, 8, 9, 10, 11, 12)
combined_matrix <- rbind(vector1, vector2)
array_3d <- array(combined_matrix, dim = c(3, 3, 2))
print(array_3d)

#output
, , 1

     [,1] [,2] [,3]
[1,]    1    8    4
[2,]    7    3   10
[3,]    2    9    5

, , 2

     [,1] [,2] [,3]
[1,]   11    1    8
[2,]    6    7    3
[3,]   12    2    9


df <- data.frame(A = 1:3, B = 4:6, C = 7:9)
matrix_from_df <- as.matrix(df)
matrix_from_df
transpose_matrix <- function(mat) {
  return(t(mat))
}
transposed <- transpose_matrix(matrix_from_df)
transposed


#output
     A B C
[1,] 1 4 7
[2,] 2 5 8
[3,] 3 6 9
  [,1] [,2] [,3]
A    1    2    3
B    4    5    6
C    7    8    9
